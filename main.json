{
  "id": "main",

  "inputs": "src/main.js",
  "paths": ["src"],

  "define": {
    "goog.DEBUG": false
  },

  "mode": "ADVANCED",
  "level": "VERBOSE",

  "checks": {
    // Unfortunately, the Closure Library violates these in many places.
    // "accessControls": "ERROR",
    // "visibility": "ERROR"

    "checkTypes": "ERROR",
    "checkRegExp": "ERROR",
    "checkVars": "ERROR",
    "deprecated": "ERROR",
    "fileoverviewTags": "ERROR",
    "invalidCasts": "ERROR",
    "missingProperties": "ERROR",
    "nonStandardJsDocs": "ERROR",
    "undefinedVars": "ERROR"
  }
}
